source 'https://rubygems.org'

gem 'rails',          '3.2.8'

platforms :ruby do
  gem 'mysql2',         '0.3.11'
end
# If running on JRuby, use the activerecord-jdbc-adapter
platforms :jruby do
  gem 'activerecord-jdbc-adapter', :require => false
end

group :assets do
  gem 'sass-rails',   '3.2.5'
  gem 'coffee-rails', '3.2.2'
  gem 'uglifier',     '1.2.3'
end

gem 'jquery-rails',   '2.0.2'

